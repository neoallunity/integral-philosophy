digraph site_structure {\n  rankdir=TB;\n  splines=ortho;\n  node [shape=box, style=rounded, fontname="Arial"];\n  edge [fontname="Arial", fontsize=10];\n\n  label = "Site Structure: https://example.com";\n  labelloc = "t";\n\n  example_com [label="example.com", fillcolor="#757575", shape="rectangle", style="filled"];\n  example_com_about [label="about", fillcolor="#00BCD4", shape="('box', 'style=rounded')", style="filled"];\n  example_com_contact [label="contact", fillcolor="#00BCD4", shape="('box', 'style=rounded')", style="filled"];\n\n  example_com -> example_com_about [label="link"];\n  example_com -> example_com_contact [label="link"];\n  example_com_about -> example_com [label="link"];\n  example_com_contact -> example_com [label="link"];\n}